<view class="cu-list menu sm-border margin-top"><view class="cu-list menu-avatar"><view class="cu-item "><block wx:if="{{userInfo!=null}}"><view class="cu-avatar radius lg" style="{{('background-image:url('+userInfo.avatarUrl+');')}}"></view></block><block wx:if="{{userInfo!=null}}"><view class="content"><view class="text-pink"><view class="text-cut">{{userInfo.nickName}}</view></view><view class="text-gray text-sm flex"><view class="text-cut">{{userInfo.country+' '+userInfo.city}}</view></view></view></block><block wx:else><view class="content"><button class="cu-btn lines-cyan round shadow" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">登录</button></view></block><view class="action"><view class="text-grey text-xs">今日未完成</view><view class="cu-tag round bg-red sm">{{userInfo.ucplans}}</view></view></view></view><view class="cu-item arrow margin-top"><view data-event-opts="{{[['tap',[['toMyDiary',['$event']]]]]}}" class="content" bindtap="__e"><text class="cuIcon-edit text-grey"></text><text class="text-grey">我的日记</text></view></view><view class="cu-item arrow"><view data-event-opts="{{[['tap',[['toMyPlan',['$event']]]]]}}" class="content" bindtap="__e"><text class="cuIcon-notice text-grey"></text><text class="text-grey">我的计划</text></view></view><view class="cu-item arrow"><view data-event-opts="{{[['tap',[['toMyCountDown',['$event']]]]]}}" class="content" bindtap="__e"><text class="cuIcon-countdown text-grey"></text><text class="text-grey">我的倒计时</text></view></view><view class="cu-item arrow"><view class="content"><text class="cuIcon-mail text-grey"></text><text class="text-grey">我的信件</text></view></view></view>